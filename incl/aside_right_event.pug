						.nm-tab-content#statistics-tab
							.nm-tabs-wrapper
								ul.nm-tabs-list.nm-small-tabs.nm-statistics-tabs
									li.active: a(href="javascript:void(0)") All
									li: a(href="javascript:void(0)") 1ST
									li: a(href="javascript:void(0)") 2ND

								.nm-tabs-content
									mixin stat(l, m, r, leftbets, rightbest)
										.nm-statistic-row
											div(class= leftbets ? "nm-best" : "")
												if ((i==0)||!(l>0))
													span= l
												if ((i==1)&&(l>0))
													span= ++l
												if ((i==2)&&(l>0))
													span= --l
											div
												span= m
											div(class= rightbest ? "nm-best" : "")
												if ((i==0)||!(r>0))
													span= r
												if ((i==1)&&(r>0))
													span= ++r
												if ((i==2)&&(r>0))
													span= --r

									- for (i=0; i<3; i++) {
										div(class="nm-tab-content" + ((i==0) ? " active" : ""))
											.nm-statistics-section
												.nm-statistics-block
													+stat("47%", "Ball possession", "53%", "", "")

												.nm-statistics-block
													+stat("13", "Total shot", "10", "+", "")
													+stat("13", "Shots on target ", "10", "+", "")
													+stat("13", "Shots off target ", "13", "", "")
													+stat("3", " Blocked shots ", "12", "", "+")

												.nm-statistics-block
													+stat("2", "Shots inside box", "4", "", "+")
													+stat("6", "Shots outside box", "6", "", "")
													+stat("6", "Big canses created", "6", "", "")
													+stat("13", "Hit woodwork", "10", "+", "")
													+stat("13", "Goalkeepers saves", "10", "+", "")

												.nm-statistics-block
													+stat("6", "Corner kicks", "2", "+", "")
													+stat("1", "Passes", "4", "", "+")
													+stat("6", "Accurate passes", "6", "", "")
													+stat("75%", "Accurate passes %", "75%", "", "")

												.nm-statistics-block
													+stat("0", "Offsides", "0", "", "")
													+stat("4", "Fouls", "4", "", "")
													+stat("2", "Yellow cards", "2", "", "")

												.nm-statistics-block
													+stat("6", "Duels won", "6", "", "")
													+stat("2", "Aerials won", "2", "", "")
									-}


