                                                    .nm-tabs-content
                                                        mixin stat(l, m, r, leftbets, rightbest)
                                                            .nm-statistic-row
                                                                div(class= leftbets ? "nm-best" : "")
                                                                    if ((i == 0) || !(l > 0))
                                                                        span= l
                                                                    if ((i == 1) && (l > 0))
                                                                        span= ++l
                                                                    if ((i == 2) && (l > 0))
                                                                        span= --l
                                                                div
                                                                    span= m
                                                                div(class= rightbest ? "nm-best" : "")
                                                                    if ((i == 0) || !(r > 0))
                                                                        span= r
                                                                    if ((i == 1) && (r > 0))
                                                                        span= ++r
                                                                    if ((i == 2) && (r > 0))
                                                                        span= --r

                                                        - for (i=0; i<3; i++) {
                                                        div(class="nm-tab-content" + ((i == 0) ? " active" : ""))
